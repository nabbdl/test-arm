{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "variables": {},
    "parameters": {
          "routeTablesArray": {
              "type": "array"
          }
    },
    "resources": [
        {
            "apiVersion": "2017-05-10",
            "name": "[concat('Deploy-',parameters('routeTablesArray')[copyIndex('routeTables')].RouteName)]",
            "type": "Microsoft.Resources/deployments",
            "resourceGroup": "[parameters('routeTablesArray')[copyIndex('routeTables')].ResourceGroupName]",
            "copy": {
                "name": "routeTables",
                "count": "[length(parameters('routeTablesArray'))]",
                "mode": "Parallel"
            },
            "properties": {
              "mode": "Incremental",
              "template": {
                  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
                  "contentVersion": "1.0.0.0",
                  "parameters": {},
                  "variables": {},
                  "resources": [
                       {
                           "type": "Microsoft.Network/routeTables",
                           "apiVersion": "2016-06-01",
                           "location": "[parameters('routeTablesArray')[copyIndex()].location]",
                           "name": "[parameters('routeTablesArray')[copyIndex()].name]",
                           "properties": "[parameters('routeTablesArray')[copyIndex()].properties]",
                           "tags": "[parameters('routeTablesArray')[copyIndex()].tags]"
                       }
                  ]
               }
            }
        }    
    ],
    "outputs": {}
  }
